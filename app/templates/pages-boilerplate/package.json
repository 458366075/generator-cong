  "dependencies": {
    "bootstrap": "4.4.1",
    "jquery": "3.4.1",
    "popper.js": "1.16.1"
  },
  "devDependencies": {
    "cong-pages": "^1.0.1"
  },
  "engines": {
    "node": ">=6"
  }
